{
  "name": "pdf-fonts",
  "version": "1.0.0",
  "description": "this module to host diff pdf fonts for pdfmake module",
  "main": "index.js",
  "scripts": {
    "prestart": "npm install",
    "start": "./node_modules/.bin/babel-node index.js",
    "build": "babel source --presets babel-preset-es2015 --out-dir distribution",
    "test": "mocha --compilers js:babel-core/register  test/ --recursive --require babel-polyfill"
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LBenzahia/pdf-fonts.git"
  },
  "keywords": [
    "pdfmake",
    "pdfkit",
    "nodejs",
    "es6",
    "pdf",
    "fonts"
  ],
  "author": "Lakhdar Benzahia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LBenzahia/pdf-fonts/issues"
  },
  "homepage": "https://github.com/LBenzahia/pdf-fonts#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-flow": "^1.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.4.0",
    "isparta": "^4.0.0",
    "mocha": "^3.5.3"
  }
}
